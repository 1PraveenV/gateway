{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "audit": {
      "type": "application",
      "root": "apps/audit",
      "entryFile": "main",
      "sourceRoot": "apps/audit/src",
      "compilerOptions": {
        "tsConfigPath": "apps/audit/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "des-form": {
      "type": "application",
      "root": "apps/des-form",
      "entryFile": "main",
      "sourceRoot": "apps/des-form/src",
      "compilerOptions": {
        "tsConfigPath": "apps/des-form/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "application",
      "root": "apps/notification",
      "entryFile": "main",
      "sourceRoot": "apps/notification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification/tsconfig.app.json"
      }
    },
    "organization": {
      "type": "application",
      "root": "apps/organization",
      "entryFile": "main",
      "sourceRoot": "apps/organization/src",
      "compilerOptions": {
        "tsConfigPath": "apps/organization/tsconfig.app.json"
      }
    },
    "project": {
      "type": "application",
      "root": "apps/project",
      "entryFile": "main",
      "sourceRoot": "apps/project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/project/tsconfig.app.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "rmq": {
      "type": "library",
      "root": "libs/rmq",
      "entryFile": "index",
      "sourceRoot": "libs/rmq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rmq/tsconfig.lib.json"
      }
    },
    "singhealth": {
      "type": "application",
      "root": "apps/singhealth",
      "entryFile": "main",
      "sourceRoot": "apps/singhealth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/singhealth/tsconfig.app.json"
      }
    }
  }
}